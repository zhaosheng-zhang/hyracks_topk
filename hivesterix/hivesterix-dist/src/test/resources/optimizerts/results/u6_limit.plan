write [%0->$$1, %0->$$4, %0->$$5, %0->$$7]
-- SINK_WRITE  |UNPARTITIONED|
  limit 4
  -- STREAM_LIMIT  |UNPARTITIONED|
    limit 4
    -- STREAM_LIMIT  |UNPARTITIONED|
      exchange 
      -- SORT_MERGE_EXCHANGE [$$4(ASC) ]  |PARTITIONED|
        limit 4
        -- STREAM_LIMIT  |PARTITIONED|
          exchange 
          -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
            order (ASC, %0->$$4) 
            -- STABLE_SORT [$$4(ASC)]  |PARTITIONED|
              exchange 
              -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
                select (function-call: algebricks:lt, Args:[%0->$$4, 10000.0])
                -- STREAM_SELECT  |PARTITIONED|
                  exchange 
                  -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
                    data-scan [$$1, $$4, $$5, $$7]<-[$$1, $$2, $$3, $$4, $$5, $$6, $$7, $$8, $$9] <- default.orders
                    -- DATASOURCE_SCAN  |PARTITIONED|
                      exchange 
                      -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
                        empty-tuple-source
                        -- EMPTY_TUPLE_SOURCE  |PARTITIONED|
